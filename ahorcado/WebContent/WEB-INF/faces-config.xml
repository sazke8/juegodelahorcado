<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>com.Ahorcado.Services.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>main2</managed-bean-name>
		<managed-bean-class>com.Ahorcado.Controllers.MainGUI</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-action>registrar</from-action>
			<from-outcome>registrar</from-outcome>
			<to-view-id>/registroJugador.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>registroJugador</display-name>
		<from-view-id>/registroJugador.jsp</from-view-id>
		<navigation-case>
			<from-action>regresa</from-action>
			<from-outcome>regresa</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-action>entrar</from-action>
			<from-outcome>entrar</from-outcome>
			<to-view-id>/iniciarSesion.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>iniciarSesion</display-name>
		<from-view-id>/iniciarSesion.jsp</from-view-id>
		<navigation-case>
			<from-action>regresa</from-action>
			<from-outcome>regresa</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
